#!/bin/env szl
#szl_options

#desc: sawzall can interpret and print UTF-8 charset.

#inst: string literal in different charset.
utf_string0: string = "Türkçe: Merhaba"; 
emit stdout <- utf_string0;

utf_string0 = "Español: ¡Hola!";
emit stdout <- utf_string0;

utf_string0 = "Ηελλένικα: Γειά σας";
emit stdout <- utf_string0;

utf_string0 = "Русский: Здравствуйте!";
emit stdout <- utf_string0;

utf_string0 = "﻿日本語: 明朝";
emit stdout <- utf_string0;
