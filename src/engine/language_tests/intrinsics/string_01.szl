#!/bin/env szl
#szl_options

#desc: general string intrinsic function call.

#inst: lowercase, uppercase
s0: string = "abc";
s1: string = "ABC";
s2: string = "ABC123";

assert(lowercase(s0) == "abc");
assert(lowercase(s1) == "abc");
assert(lowercase(s2) == "abc123");

assert(uppercase(s0) == "ABC");
assert(uppercase(s1) == "ABC");
assert(uppercase(s2) == "ABC123");

