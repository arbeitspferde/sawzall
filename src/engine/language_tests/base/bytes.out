s1: bytes;
s2: bytes;
s3: bytes;
s1 = convert(bytes, "日本語");
s2 = convert(bytes, "日本語");
assert(s1 == s2);
assert((s2 + s2) == (s1 + s1));
assert(((s1[0 : 1] + s1[1 : 2]) + s1[2 : 3]) != s2);
assert(s1[0 : 0] == convert(bytes, ""));
assert(s1[0] != 26085);
s3 = s1;
s3[0] = 88;
assert(s1[0] != 26085);
assert(s3[0] == 88);
assert(s1[1 : 3] == s2[1 : 3]);
assert(s1[1 : 3] != s1[1 : len(s1)]);
assert(s1[1 : $] == s1[1 : len(s1)]);
assert(s1[0 : $] == s1);
assert(s1[$ - $ : $] == s1);
assert(s1[$ - $ : $ / 2] != s1[0 : 1]);
s4: bytes = {127, 128, 129, 255};
assert(s4[0] == 127);
assert(s4[1] == 128);
assert(s4[2] == 129);
assert(s4[3] == 255);
