#!/bin/env szl

#szl_options -noexecute

#desc: Different declarations for the Sawzall variables

### 	Variable declarations:
#  	Grammar:
#  1) 	VarCal := ['static'] var_name ':' Type [ '=' Expression | block ] ';'
#  2)	var_name := identifier
#  3)	identifier := (letter | '_') { letter | '_' | digit }
#  4)	letter =: 'A' ... 'Z' | 'a' ... 'z' 
#  5)	digit =: '0' .. '9'

#inst: Variable declaration without initialization
$%__asfd12sa@: float;  	# identifier should start with a letter or '_'
ASDF:int;
(213,(ASFD : string ;
sad_#:int;

~!@#$%^&*()_+:string;


