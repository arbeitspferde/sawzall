#!/bin/env  szl

#desc:  Conversion from composite to basic type


# conversion to int

i0:int = int({1,2,3,4});				# array of int
i1:int = int({1:2,2:3,3:4,4:5});			# map[int] of int
i2:int = int({{1,"Hi"},{2,"World"}});			# tuple {int,string}

# conversion to float

f0:float = float({1,2,3,4});				# array of int
f1:float = float({1:2,2:3,3:4,4:5});			# map[int] of int
f2:float = float({{1,"Hi"},{2,"World"}});		# tuple {int,string}

# conversion to bool

b0:bool = bool({1,2,3,4});				# array of int
b1:bool = bool({1:2,2:3,3:4,4:5});			# map[int] of int
b2:bool = bool({{1,"Hi"},{2,"World"}});		# tuple {int,string}

# conversion to bytes

by0:bytes = bytes({1,2,3,4});				# array of int
by1:bytes = bytes({1:2,2:3,3:4,4:5});			# map[int] of int
by2:bytes = bytes({{1,"Hi"},{2,"World"}});		# tuple {int,string}

# conversion to time

t0:time = time({1,2,3,4});				# array of int
t1:time = time({1:2,2:3,3:4,4:5});			# map[int] of int
t2:time = time({{1,"Hi"},{2,"World"}});		# tuple {int,string}
