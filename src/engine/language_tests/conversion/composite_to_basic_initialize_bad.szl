#!/bin/env  szl

#desc:  Conversion from composite to basic type


# conversion to int

i0:int = {1,2,3,4};				# array of int
i1:int = {1:2,2:3,3:4,4:5};			# map[int] of int
i2:int = {{1,"Hi"},{2,"World"}};		# tuple {int,string}

# conversion to float

f0:float = {1,2,3,4};				# array of int
f1:float = {1:2,2:3,3:4,4:5};			# map[int] of int
f2:float = {{1,"Hi"},{2,"World"}};		# tuple {int,string}

# conversion to string

s0:string = {1,2,3,4};				# array of int
s1:string = {1:2,2:3,3:4,4:5};			# map[int] of int
s2:string = {{1,"Hi"},{2,"World"}};		# tuple {int,string}

# conversion to bool

b0:bool = {1,2,3,4};				# array of int
b1:bool = {1:2,2:3,3:4,4:5};			# map[int] of int
b2:bool = {{1,"Hi"},{2,"World"}};		# tuple {int,string}

# conversion to bytes

by0:bytes = {1,2,3,4};				# array of int
by1:bytes = {1:2,2:3,3:4,4:5};			# map[int] of int
by2:bytes = {{1,"Hi"},{2,"World"}};		# tuple {int,string}

# conversion to fingerprint

fg0:fingerprint = {1,2,3,4};				# array of int
fg1:fingerprint = {1:2,2:3,3:4,4:5};			# map[int] of int
fg2:fingerprint = {{1,"Hi"},{2,"World"}};		# tuple {int,string}

# conversion to time

t0:time = {1,2,3,4};				# array of int
t1:time = {1:2,2:3,3:4,4:5};			# map[int] of int
t2:time = {{1,"Hi"},{2,"World"}};		# tuple {int,string}
