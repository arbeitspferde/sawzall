x:int;
s:string = "%d";

x = DEBUG("foo");
x = DEBUG("print");
x = DEBUG("print", "%d");
x = DEBUG("print", s, 3);
x = DEBUG("print", "%s", 3);
x = DEBUG("print", "%s", "hello", 3);
x = DEBUG("print", "%");
x = DEBUG("print", "%A", 3);
x = DEBUG("print", "%.*s", 3, "hello");
x = DEBUG("print", "%n", 4);
x = DEBUG("print", "%lld", 4);
