#!/bin/env

#desc:  different treatment to the bytes literal value

by0:bytes;
by0 = {'a','b','c'};
if(by0 == bytes({'a','b','c'}))
                                                                                                 
    DEBUG("print","This works");

if(by0 == {'a','b','c'})

    DEBUG("print","This works");

by1:bytes;
by1 = {1,2,3};
if(by1 == {1,2,3} )
    DEBUG("print","This works");


